#Windows manager
  sudo pacman -S --noconfirm --needed --verbose bspwm sxhkd xdo xorg-twmn
#Config
  sudo install -Dm755 /usr/share/doc/bspwm/examples/bspwmrc /home/$USER/.config/bspwm/bspwmrc -v
  sudo install -Dm644 /usr/share/doc/bspwm/examples/sxhkdrc /home/$USER/.config/sxhkd/sxhkdrc -v
  sudo echo "expec bspwm" > /home/$USER/.xinitrc
  sudo systemctl enable sxhkd
#Bar 
 sudo pacman -S --noconfirm --needed --verbose polybar
#Terminal
  sudo pacman -S --noconfirm --needed --verbose xterm qterminal kitty alacritty picom jq
#Audio
  sudo pacman -S --noconfirm --needed --verbose pulseaudio pavucontrol cmus mpv mpd ncmpcpp
#Image and filles manager
  sudo pacman -S --noconfirm --needed --verbose viewnior nemo ranger scrot feh ueberzug imagemagick
#System monitors
  sudo pacman -S --noconfirm --needed --verbose htop rofi neofetch conky jq       
#Fonts and icons
  sudo pacman -S --noconfirm --needed --verbose ttf-nerd-fonts-symbols gnu-free-fonts ttf-dejavu ttf-nerd-fonts-symbols    
unzip  ~/Downloads/JetBrainsMono-2.304.zip ./
cp  ~/Downloads/JetBrainsMono-2.304 /usr/share/fonts
fc-cache -f -v
#Browser
  sudo pacman -S --noconfirm --needed --verbose w3m
#dots
  cd ~/git/
  git clone https://gitlab.com/prolinux410/owl_dots
  cat ~/owl_dots/bspwm/bspwm_yellow/.config >> ~/.config
    #~/owl_dots/bspwm/bspwm_otto/.config

  F_CF=~/.xinitrc
  echo "exec sxhkd -c ~/.config/bspwm/themes/bspwm_yellow/sxhkd/sxhkdrc &" > $F_CF
  echo "exec bspwm -c ~/.config/bspwm/themes/bspwm_yellow/bspwm/bspwmrc" > $F_CF
